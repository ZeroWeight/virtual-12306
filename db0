/*
 *	database.cpp
 */

#include "database.h"

Database::Database(void)
{
	db = QSqlDatabase::addDatabase("QSQLITE");
	if (false) // Init people.db.T_Passenger
	{
		db.setDatabaseName("people.db");
		db.open();
		QSqlQuery query(db);
		query.exec("DROP TABLE T_Passenger");
		query.exec("PRAGMA foreign_keys = ON");
		query.exec("CREATE TABLE T_Passenger ("
			"Psngr_Name VARCHAR(30) NOT NULL,"
			"Psngr_Gender CHAR(1) NOT NULL,"
			"Psngr_IdNumber CHAR(18) NOT NULL PRIMARY KEY,"
			"Psngr_Phone VARCHAR(20) NOT NULL,"
			"Psngr_PassengerType INT NOT NULL,"
			"Psngr_University VARCHAR(100),"
			"Psngr_StudentId VARCHAR(20),"
			"Psngr_AdmissionYear INT,"
			"Psngr_Range1 VARCHAR(15),"
			"Psngr_Range2 VARCHAR(15))");
		db.close();
	}
	if (false) // Init people.db.T_User
	{
		db.setDatabaseName("people.db");
		db.open();
		QSqlQuery query(db);
		query.exec("DROP TABLE T_User");
		query.exec("CREATE TABLE T_User ("
			"Usr_Username VARCHAR(30) NOT NULL PRIMARY KEY,"
			"Usr_Password CHAR(64) NOT NULL,"
			"Usr_Auth BOOLEAN NOT NULL DEFAULT FALSE,"
			"Usr_Balance INT NOT NULL DEFAULT 0,"
			"Usr_Contact0 CHAR(18) REFERENCES T_Passenger(Psngr_IdNumer) ON UPDATE CASCADE,"
			"Usr_Contact1 CHAR(18) REFERENCES T_Passenger(Psngr_IdNumer) ON UPDATE CASCADE,"
			"Usr_Contact2 CHAR(18) REFERENCES T_Passenger(Psngr_IdNumer) ON UPDATE CASCADE,"
			"Usr_Contact3 CHAR(18) REFERENCES T_Passenger(Psngr_IdNumer) ON UPDATE CASCADE)");
		query.exec("INSERT INTO T_User (Usr_Username, Usr_Password, Usr_Auth)"
			"VALUES ('Administrator', 'a529dfcdd72cd20f1dd17160976bc3b5db6073c538a1c6f1a7439daa1b63ce76', 'TRUE')");
		db.close();
	}
	if (false) // Init people.db.T_Ticket
	{
		db.setDatabaseName("people.db");
		db.open();
		QSqlQuery query(db);
		query.exec("DROP TABLE T_Ticket");
		query.exec("PRAGMA foreign_keys = ON");
		query.exec("CREATE TABLE T_Ticket ("
			"Tckt_Date CHAR(8) NOT NULL,"
			"Tckt_TrainId VARCHAR(5) NOT NULL,"
			"Tckt_DepartureStation VARCHAR(15) NOT NULL,"
			"Tckt_ArrivalStation VARCHAR(15) NOT NULL,"
			"Tckt_DepartureTime CHAR(4) NOT NULL,"
			"Tckt_ArrivalTime CHAR(4) NOT NULL,"
			"Tckt_Price INT NOT NULL,"
			"Tckt_Seat INT NOT NULL,"
			"Tckt_Passenger CHAR(18) NOT NULL REFERENCES T_Passenger(Psngr_IdNumer))");
		db.close();
	}
	if (false) // Init train.db.T_Station
	{
		db.setDatabaseName("train.db");
		db.open();
		QSqlQuery query(db);
		query.exec("DROP TABLE T_Station");
		query.exec("CREATE TABLE T_Station ("
			"Stn_Name VARCHAR(15) NOT NULL PRIMARY KEY,"
			"Stn_TrainNum INT NOT NULL,"
			"Stn_Train0 VARCHAR(5),"
			"Stn_Train1 VARCHAR(5),"
			"Stn_Train2 VARCHAR(5),"
			"Stn_Train3 VARCHAR(5),"
			"Stn_Train4 VARCHAR(5),"
			"Stn_Train5 VARCHAR(5),"
			"Stn_Train6 VARCHAR(5),"
			"Stn_Train7 VARCHAR(5),"
			"Stn_Train8 VARCHAR(5),"
			"Stn_Train9 VARCHAR(5))");
		query.exec("INSERT INTO T_Station VALUES ('BEIJING', 10, 'D901', 'G101', 'G355', 'G41', 'G43', 'G71', 'K571', 'K599', 'Z307', 'Z35')");
		query.exec("INSERT INTO T_Station VALUES ('CHANGSHA', 10, 'G1003', 'G1692', 'G279', 'G433', 'G71', 'K599', 'K9075', 'T368', 'Z335', 'Z35')");
		query.exec("INSERT INTO T_Station (Stn_Name, Stn_TrainNum, Stn_Train0, Stn_Train1, Stn_Train2, Stn_Train3, Stn_Train4, Stn_Train5, Stn_Train6, Stn_Train7)"
			"VALUES ('FUZHOU', 8, 'D2244', 'D2287', 'D3141', 'D3145', 'D3231', 'D3263', 'G1692', 'G355')");
		query.exec("INSERT INTO T_Station VALUES ('GUANGZHOU', 10, 'D901', 'D941', 'G1003', 'G279', 'G71', 'K298', 'K599', 'K9075', 'T368', 'Z35')");
		query.exec("INSERT INTO T_Station VALUES ('HANGZHOU', 10, 'D2287', 'D3141', 'D3145', 'D3231', 'D941', 'G41', 'G43', 'K1209', 'K1805', 'T135')");
		query.exec("INSERT INTO T_Station VALUES ('JINAN', 10, 'G101', 'G279', 'G355', 'G41', 'G43', 'G433', 'K1053', 'K293', 'K849', 'T135')");
		query.exec("INSERT INTO T_Station (Stn_Name, Stn_TrainNum, Stn_Train0, Stn_Train1, Stn_Train2, Stn_Train3, Stn_Train4, Stn_Train5, Stn_Train6)"
			"VALUES ('NANJING', 7, 'G101', 'G41', 'G43', 'K1805', 'K849', 'T135', 'Z4515')");
		query.exec("INSERT INTO T_Station VALUES ('SHANGHAI', 10, 'D2287', 'D3141', 'D3145', 'D941', 'G101', 'G41', 'G43', 'K1209', 'K1805', 'T135')");
		query.exec("INSERT INTO T_Station (Stn_Name, Stn_TrainNum, Stn_Train0, Stn_Train1, Stn_Train2, Stn_Train3, Stn_Train4, Stn_Train5, Stn_Train6)"
			"VALUES ('SHENZHEN', 7, 'D2287', 'D901', 'D941', 'G1003', 'G279', 'G71', 'K9075')");
		query.exec("INSERT INTO T_Station VALUES ('SHIJIAZHUANG', 10, 'G1276', 'G1292', 'G363', 'G6277', 'G71', 'K1805', 'K599', 'K849', 'T368', 'Z335')");
		query.exec("INSERT INTO T_Station (Stn_Name, Stn_TrainNum, Stn_Train0, Stn_Train1, Stn_Train2, Stn_Train3, Stn_Train4, Stn_Train5, Stn_Train6, Stn_Train7, Stn_Train8)"
			"VALUES ('TIANJIN', 9, 'G1276', 'G1292', 'G355', 'G363', 'G6277', 'K1053', 'K293', 'T368', 'Z4515')");
		query.exec("INSERT INTO T_Station VALUES ('WUHAN', 10, 'D2244', 'D3263', 'G1003', 'G1276', 'G1292', 'G279', 'G363', 'G433', 'G71', 'Z335')");
		query.exec("INSERT INTO T_Station VALUES ('XIAMEN', 10, 'D2287', 'D3141', 'D3145', 'D3231', 'G1692', 'G355', 'K1209', 'K298', 'K571', 'Z307')");
		query.exec("INSERT INTO T_Station (Stn_Name, Stn_TrainNum, Stn_Train0, Stn_Train1, Stn_Train2, Stn_Train3, Stn_Train4, Stn_Train5, Stn_Train6, Stn_Train7, Stn_Train8)"
			"VALUES ('XUZHOU', 9, 'G101', 'G279', 'G355', 'G43', 'G433', 'K1805', 'K849', 'T135', 'Z4515')");
		db.close();
	}
	if (false)
