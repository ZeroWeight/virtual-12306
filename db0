			"VALUES (3, 'NANJING', '1230', 136, 233)");
		
		db.close();
	}
	if (false)
	{
		db.setDatabaseName("train.db");
		db.open();
		QSqlQuery query(db);
		query.exec("DROP TABLE T_Log");
		query.exec("CREATE TABLE T_Log ("
			"Lg_TheOtherDay TEXT NOT NULL,"
			"Lg_Today TEXT NOT NULL)");
		query.exec("INSERT INTO T_Log VALUES (date('now'), date('now'))");
		db.close();
	}

	db.setDatabaseName("train.db");
	db.open();
	QSqlQuery query(db);
	query.exec("UPDATE T_Log SET Lg_Today = date('now')");
	db.close();
}

Database::~Database(void)
{
	db.setDatabaseName("train.db");
	db.open();
	QSqlQuery query(db);
	query.exec("UPDATE T_Log SET Lg_TheOtherDay = date('now')");
	db.close();
}

void Database::Login(const QString &username, const QString &password)
{
	qDebug() << username;
	qDebug() << password;
	db.setDatabaseName("people.db");
	db.open();
	QSqlQuery query(db);
	query.prepare("SELECT * FROM T_User WHERE Usr_Username = ?");
	query.addBindValue(username);
	query.exec();
	QSqlRecord rec = query.record();
	if (rec.isEmpty())
	{
		qDebug() << "User does not exist.";
	}
	else
	{
		while (query.next())
		{
			if (query.value(rec.indexOf("Usr_Password")).toString() != password)
			{
				qDebug() << "Wrong password.";
			}
			else
			{
				if (query.value(rec.indexOf("Usr_Auth")).toBool())
				{
					qDebug() << "Admin login.";
				}
				else
				{
					qDebug() << "User login.";
				}
			}
		}
	}
	db.close();
}

void Database::Register(const QString &name, const QString &gender, const QString &idNumber, const QString &phone, const int &passengerType, const QString &university, const QString &studentId, const int &admissionYear, const QString &range1, const QString &range2, const QString &username, const QString &password)
{
	db.setDatabaseName("people.db");
	db.open();
	QSqlQuery query(db);
	query.prepare("SELECT Usr_Username FROM T_User WHERE Usr_Username = ?");
	query.addBindValue(username);
	query.exec();
	if (!query.record().isEmpty())
	{
		qDebug() << "Username exists.";
	}
	else
	{
		query.prepare("SELECT Psngr_IdNumber FROM T_Passenger WHERE Psngr_IdNumber = ?");
		query.addBindValue(idNumber);
		query.exec();
		if (!query.record().isEmpty())
		{
			qDebug() << "ID number exists.";
		}
		else
		{
			query.prepare("INSERT INTO T_Passenger VALUES (?, ?, ?, ?, ?, ?, ?, ?, ?, ?)");
			query.addBindValue(name);
			query.addBindValue(gender);
			query.addBindValue(idNumber);
			query.addBindValue(phone);
			query.addBindValue(passengerType);
			query.addBindValue(university);
			query.addBindValue(studentId);
			query.addBindValue(admissionYear);
			query.addBindValue(range1);
			query.addBindValue(range2);
			query.exec();
			query.prepare("INSERT INTO T_User (Usr_Username, Usr_Password, Usr_Contact0) VALUES (?, ?, ?)");
			query.addBindValue(username);
			query.addBindValue(password);
			query.addBindValue(idNumber);
			query.exec();
			qDebug() << "Success.";
		}
	}
	db.close();
}

void Database::UpdateInfo(const QString &name, const QString &gender, const QString &idNumber, const QString &phone, const int &passengerType, const QString &university, const QString &studentId, const int &admissionYear, const QString &range1, const QString &range2, const QString &username, const QString &password)
{
	db.setDatabaseName("people.db");
	db.open();
	QSqlQuery query(db);
	query.prepare("UPDATE T_Passenger"
		"SET Psngr_Gender = ?,"
		"Psngr_Phone = ?,"
		"Psngr_PassengerType = ?,"
		"Psngr_University = ?,"
		"Psngr_StudentId = ?,"
		"Psngr_AdmissionYear = ?,"
		"Psngr_Range1 = ?,"
		"Psngr_Range2 = ?"
		"WHERE Psngr_IdNumber = ?");
	query.addBindValue(gender);
	query.addBindValue(phone);
	query.addBindValue(passengerType);
	query.addBindValue(university);
	query.addBindValue(studentId);
	query.addBindValue(admissionYear);
	query.addBindValue(range1);
	query.addBindValue(range2);
	query.addBindValue(idNumber);
	query.exec();
	query.prepare("UPDATE T_User SET Usr_Password = ? WHERE Usr_Username = ?");
	query.addBindValue(password);
	query.addBindValue(username);
	query.exec();
	qDebug() << "Success.";
}
